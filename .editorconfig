# EditorConfig: https://EditorConfig.org

# Plugins -> Plugins Admin -> EditorConfig

# top-most EditorConfig file
root = true

[*]
trim_trailing_whitespace = true
insert_final_newline = true

# Indentation override for all terraform files
[*.{tf, tfvars}]
indent_style = space
indent_size = 2
trim_trailing_whitespace = true
insert_final_newline = true
